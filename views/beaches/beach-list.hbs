<div class="container">
  <h2>Listado de playas {{beach.Nombre}}</h2>
  <hr />

  <ul class="beach-list">
    {{#each beach}}
      <li><a
          href="/playa/detalles/{{attributes.Identifica}}"
        >{{attributes.Nombre}}</a></li>
    {{/each}}
  </ul>

  <a href="/playa/listado?pagina={{nextPage}}" class="next-button">Siguiente</a>

</div>